<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>User Home Page</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
    <link rel="stylesheet" type="text/css" href="https://use.fontawesome.com/releases/v5.4.1/css/all.css">
    <!-- <link rel="stylesheet" type="text/css" href="/public/ash.css"> -->
    <style>
    body {
    background: #2f585f;  /* fallback for old browsers */
    background: -webkit-linear-gradient(to right, #ffd194, #70e1f5);  /* Chrome 10-25, Safari 5.1-6 */
    background: linear-gradient(to right, #ffd194, #70e1f5); /* W3C, IE 10+/ Edge, Firefox 16+, Chrome 26+, Opera 12+, Safari 7+ */

}

#mainContainer {
    margin-top: 80px;
}

.container {
    max-width: 60rem;
}

.jumbotron {
    background-color: inherit ;
}

.list-group-item {
    background: #e9ecef;
    margin: 1%;
}

.list-group-item.active {
    background: #70e1f5;  /* fallback for old browsers */
    background: -webkit-linear-gradient(to right, #ffd194, #70e1f5);  /* Chrome 10-25, Safari 5.1-6 */
    background: linear-gradient(to right, #ffd194, #70e1f5); /* W3C, IE 10+/ Edge, Firefox 16+, Chrome 26+, Opera 12+, Safari 7+ */
    transition: background 500ms;
    color: black;
}

.well {
    min-height: 20px;
    padding: 19px;
    margin-bottom: 20px;
    background: #e9ecef;
    border: 1px solid #e3e3e3;
    border-radius: 4px;
    -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, .05);
            box-shadow: inset 0 1px 1px rgba(0, 0, 0, .05);
  }
  .well blockquote {
    border-color: #ddd;
    border-color: rgba(0, 0, 0, .15);
  }
  .well-lg {
    padding: 24px;
    border-radius: 6px;
  }
  .well-sm {
    padding: 9px;
    border-radius: 3px;
  }

#right {
    margin-left: 2rem;
}

.navbar.scrolled {
    background: #70e1f5;  /* fallback for old browsers */
    background: -webkit-linear-gradient(to right, #ffd194, #70e1f5);  /* Chrome 10-25, Safari 5.1-6 */
    background: linear-gradient(to right, #ffd194, #70e1f5); /* W3C, IE 10+/ Edge, Firefox 16+, Chrome 26+, Opera 12+, Safari 7+ */
}

@media screen and (max-width: 992px) {

    .container {
        max-width: 45rem;
    }


}

@media screen and (max-width: 768px) {

    .container {
        max-width: 30rem;
    }


}

@media screen and (max-width: 576px) {

    .container {
        /* max-height: 100px; */
        max-width: 20rem;
    }


}
    </style>
</head>
<body>

    <nav id="mainNavbar" class="navbar navbar-expand-lg navbar-light">
        <div class="container">
            <a class="navbar-brand" href="#"><i class="fas fa-user-tie"></i> <%= user.username %></a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            
            <div class="collapse navbar-collapse" id="navbarNav">
                <ul class="navbar-nav">
                        
                <li class="nav-item active">
                    <a class="nav-link" href="/home"><i class="fas fa-bus-alt"></i> Go Transport <span class="sr-only">(current)</span></a>
                </li>
                
                <li class="nav-item">
                    <a class="nav-link" href="/user/<%= user._id %>/qr" ><i class="fas fa-qrcode"></i> QR Code Scanner</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/leaderboard"><i class="fas fa-clipboard-list"></i> Leader Board</a>
                </li>
                <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><i class="fas fa-taxi"></i> 
                    Book a Cab
                </a>
                <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                    <a class="dropdown-item" href="https://book.olacabs.com/" target="_blank"><img class="logo" alt="Olacabs Logo" srcset="https://olawebcdn.com/images/v1/logos/olalogo.png 1x, https://olawebcdn.com/images/v1/logos/olalogo@2x.png 2x, https://olawebcdn.com/images/v1/logos/olalogo@3x.png 3x" src="https://olawebcdn.com/images/v1/logos/olalogo.png"></a>
                    <div class="dropdown-divider"></div>
                    <a class="dropdown-item" href="https://www.uber.com/in/en/ride/" targe="_blank"><img style="width: 50%;" class="logo img-fluid" alt="Uber Logo" src="data:image/png;base64,"></a>
                </div>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/user/<%= user._id %>/settings"><i class="fas fa-user-cog"></i> Settings</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/logout"><i class="fas fa-sign-out-alt"></i> Log Out</a>
                </li>
                </ul>
            </div>
        </div>
    </nav>

    <div id="mainContainer" class="container">
        <div class="jumbotron jumbotron-fluid row">
                <div class="col-md-4">
                    <img class="img-fluid" src="<%= user.image %>" alt="">
                </div>
                <div class="col-md-8">
                    <div class="display-4 text-center"><%= user.username %></div>
                    <div class="display-5 text-center"><%= user.email %></div>
                    <div class="display-5 text-center"><%= user.phoneNumber %></div>
                    <div class="display-5 text-center"><%= user.dob %></div>
                    <div class="lead text-center"><%= user.bio %></div>
                </div>
        </div>

    <div class="row">

        <div class="well col-sm-4"> 
            <p class="lead text-center"><i class="fas fa-coins"></i> Coins</p>
            <hr>
            <p class="lead text-center"><%= user.coins %></p>
        </div>

        <div class="col-sm-1"></div>

        <div class="well col-sm-7">

            <p class="lead text-center"><i class="far fa-address-card"></i> Posts</p>
            <hr>

            <ul class="list-group">

            <li class="list-group-item">
                <a href="/user/<%= user._id %>/newPost" class="btn btn-lg text-center lead">New Post +</p>
            </li>

            <% if(user.Post.length == 0) {%>

            <li class="list-group-item">
                <p class="text-center lead">Your Posts Will Look Like This</p>
            </li>
            <li class="list-group-item active">
                <div class="card" style="width: inherit;">
                <img src="data:image/jpeg;base64," class="card-img-top" alt="...">
                <div class="card-body">
                    <h5 class="card-title text-center lead">Post Title</h5>
                    <p class="card-text text-justify lead">Lorem ipsum dolor, sit amet consectetur adipisicing elit. Corporis, tempore!</p>
                    <a href="#" class="btn btn-outline-danger">Delete</a>
                </div>
                </div>
            </li>

            <% } else { %>

                <% user.Post.forEach(function(data){ %>
                <li class="list-group-item">
                <div class="card" style="width: inherit;">
                <img src="<%= data.image %>" class="card-img-top" alt="...">
                <div class="card-body">
                    <h5 class="card-title text-center lead"><%= data.name %></h5>
                    <p class="card-text text-justify lead"><%= data.description %></p>
                    <form action="/user/<%= user._id %>/delete/<%= data._id %>?_method=DELETE" method="POST">
                    <button class="btn btn-outline-danger">Delete</button>
                    </form>
                </div>
                </div>
            </li>
                <% }) %>
                <% } %>
        </div>

    </div>

    </div>

    <!-- CHATBOT -->
    <script src="https://static.landbot.io/landbot-widget/landbot-widget-1.0.0.js"></script>
    <script>
    var myLandbot = new LandbotLivechat({
        index: 'https://landbot.io/u/H-237423-JDZ9NF3IRHYPWXAN/index.html',
    });
    </script>
    <!-- CHATBOT END -->


    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>

